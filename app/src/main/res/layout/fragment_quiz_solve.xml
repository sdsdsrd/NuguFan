        android:layout_below="@id/button4"
        android:layout_marginTop="15dp"/>

    <ImageView
        android:layout_width="295dp"
        android:layout_height="68dp"
        android:background="@drawable/purple_box"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/button5"
        android:layout_marginTop="-68dp"/>

    <ImageView
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:background="@drawable/video"
        android:backgroundTint="@android:color/white"
        android:layout_below="@id/button5"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="-58dp"/>

    <TextView
        android:id="@+id/stextView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="종합형"
        android:textSize="16dp"
        android:textColor="#ffffff"
        android:fontFamily="@font/aritasemibold"
        android:layout_below="@id/button5"
        android:layout_marginLeft="105dp"
        android:layout_marginTop="-58dp"/>

    <TextView
        android:id="@+id/stextView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="난이도 UP ! 모든 퀴즈 유형을 MIX !"
        android:textSize="12dp"
        android:textColor="#ffffff"
        android:fontFamily="@font/aritasemibold"
        android:layout_below="@id/stextView11"
        android:layout_marginLeft="95dp"
        android:layout_marginTop="8dp"/>

    <ImageView
        android:id="@+id/lock"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/lock"
        android:layout_below="@id/button5"
        android:layout_marginTop="-55dp"
        android:layout_marginLeft="300dp"/>

    <ImageView
        android:layout_width="35dp"
        android:layout_height="8dp"
        android:background="@drawable/white_box"
        android:layout_below="@id/lock"
        android:layout_marginLeft="298dp"
        android:layout_marginTop="5dp"/>

    <ImageView
        android:layout_width="6dp"
        android:layout_height="6dp"
        android:background="@drawable/key"
        android:layout_below="@id/lock"
        android:layout_marginLeft="301dp"
        android:layout_marginTop="6dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1,000P"
        android:fontFamily="@font/aritabold"
        android:textSize="6dp"
        android:textColor="#8280AB"
        android:layout_below="@id/lock"
        android:layout_marginLeft="310dp"
        android:layout_marginTop="5dp"/>

</RelativeLayout>