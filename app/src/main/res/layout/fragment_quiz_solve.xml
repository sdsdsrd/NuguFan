<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E6E6F9">

    <ImageView
        android:id="@+id/simageView1"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:background="@drawable/gradation2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="랭킹 1위에게는 성덕 선물이 팡팡!!"
        android:textSize="20dp"
        android:textColor="#ffffff"
        android:fontFamily="@font/aritabold"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/simageView1"
        android:layout_marginTop="-41dp"/>

    <ImageView
        android:layout_width="350dp"
        android:layout_height="30dp"
        android:background="@drawable/white_box_round"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/simageView1"
        android:layout_marginTop="-10dp"/>

    <ImageView
        android:id="@+id/simageView2"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@drawable/profile"
        android:layout_below="@id/simageView1"
        android:layout_marginLeft="42dp"
        android:layout_marginTop="-5dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="아기서동 님의 오늘의 랭킹 1,465,342 위 ! 보유 포인트 0p"
        android:textColor="#7F6BFB"
        android:textSize="12dp"
        android:fontFamily="@font/aritabold"
        android:layout_marginLeft="70dp"
        android:layout_below="@id/simageView1"/>

    <ImageView
        android:id="@+id/simageView3"
        android:layout_width="350dp"
        android:layout_height="match_parent"
        android:background="@drawable/white_box"
        android:layout_below="@id/simageView2"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"/>

    <TextView
        android:id="@+id/stextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="퀴즈 유형 선택"
        android:textSize="20dp"
        android:textColor="#7F6BFB"
        android:fontFamily="@font/aritabold"
        android:layout_marginLeft="50dp"
        android:layout_above="@id/simageView3"
        android:layout_marginBottom="-70dp"/>

    <TextView
        android:id="@+id/stextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="누구팬은 다양한 퀴즈 포맷을 지원합니다.\n원하는 유형을 선택해서 재미있게 퀴즈를 풀어보세요!"
        android:textColor="#707070"
        android:textSize="13dp"
        android:layout_marginLeft="50dp"
        android:layout_below="@id/stextView1"
        android:layout_marginTop="85dp"
        android:lineSpacingExtra="8dp"/>

    <ImageButton
        android:id="@+id/button1"
        android:layout_width="295dp"
        android:layout_height="68dp"
        android:background="@drawable/imagev"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/stextView2"
        android:layout_marginTop="18dp"/>

    <ImageView
        android:layout_width="295dp"
        android:layout_height="68dp"
        android:background="@drawable/purple_box"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/button1"
        android:layout_marginTop="-68dp"/>

    <TextView
        android:id="@+id/stextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="T  텍스트형"
        android:textSize="16dp"
        android:textColor="#ffffff"
        android:fontFamily="@font/aritasemibold"
        android:layout_below="@id/button1"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="-58dp"/>

    <TextView
        android:id="@+id/stextView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="문제를 읽고 알맞은 답을 고르시오."
        android:textSize="13dp"
        android:textColor="#ffffff"
        android:fontFamily="@font/aritasemibold"
        android:layout_below="@id/stextView3"
        android:layout_marginLeft="95dp"
        android:layout_marginTop="8dp"/>

    <ImageButton
        android:id="@+id/button2"
        android:layout_width="295dp"
        android:layout_height="68dp"
        android:background="@drawable/textv"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/button1"
        android:layout_marginTop="15dp"/>

    <ImageView
        android:layout_width="295dp"
        android:layout_height="68dp"
        android:background="@drawable/purple_box"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/button2"
        android:layout_marginTop="-68dp"/>

    <ImageView
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:background="@drawable/ic_menu_gallery"
        android:backgroundTint="@android:color/white"
        android:layout_below="@id/button2"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="-58dp"/>

    <TextView
        android:id="@+id/stextView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이미지형"
        android:textSize="16dp"
        android:textColor="#ffffff"
        android:fontFamily="@font/aritasemibold"
        android:layout_below="@id/button2"
        android:layout_marginLeft="105dp"
        android:layout_marginTop="-58dp"/>

    <TextView
        android:id="@+id/stextView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="문제를 읽고 알맞은 답을 고르시오."
        android:textSize="13dp"
        android:textColor="#ffffff"
        android:fontFamily="@font/aritasemibold"
        android:layout_below="@id/stextView5"
        android:layout_marginLeft="95dp"
        android:layout_marginTop="8dp"/>

    <ImageButton
        android:id="@+id/button3"
        android:layout_width="295dp"
        android:layout_height="68dp"
        android:background="@drawable/soundv"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/button2"
        android:layout_marginTop="15dp"/>

    <ImageView
        android:layout_width="295dp"
        android:layout_height="68dp"
        android:background="@drawable/purple_box"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/button3"
        android:layout_marginTop="-68dp"/>

    <ImageView
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:background="@drawable/speaker"
        android:layout_below="@id/button3"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="-58dp"/>

    <TextView
        android:id="@+id/stextView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="사운드형"
        android:textSize="16dp"
        android:textColor="#ffffff"
        android:fontFamily="@font/aritasemibold"
        android:layout_below="@id/button3"
        android:layout_marginLeft="105dp"
        android:layout_marginTop="-58dp"/>

    <TextView
        android:id="@+id/stextView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="숨스밍했다면 1 초만 듣고도 무슨 곡인지 알겠지?"
        android:textSize="12dp"
        android:textColor="#ffffff"
        android:fontFamily="@font/aritasemibold"
        android:layout_below="@id/stextView7"
        android:layout_marginLeft="95dp"
        android:layout_marginTop="8dp"/>

    <ImageButton
        android:id="@+id/button4"
        android:layout_width="295dp"
        android:layout_height="68dp"
        android:background="@drawable/videov"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/button3"
        android:layout_marginTop="15dp"/>

    <ImageView
        android:layout_width="295dp"
        android:layout_height="68dp"
        android:background="@drawable/purple_box"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/button4"
        android:layout_marginTop="-68dp"/>

    <ImageView
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:background="@drawable/video"
        android:backgroundTint="@android:color/white"
        android:layout_below="@id/button4"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="-58dp"/>

    <TextView
        android:id="@+id/stextView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="비디오형"
        android:textSize="16dp"
        android:textColor="#ffffff"
        android:fontFamily="@font/aritasemibold"
        android:layout_below="@id/button4"
        android:layout_marginLeft="105dp"
        android:layout_marginTop="-58dp"/>

    <TextView
        android:id="@+id/stextView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="맨날 보던 우리 언니, 오빠 영상이 기출문제!"
        android:textSize="12dp"
        android:textColor="#ffffff"
        android:fontFamily="@font/aritasemibold"
        android:layout_below="@id/stextView9"
        android:layout_marginLeft="95dp"
        android:layout_marginTop="8dp"/>

    <ImageButton
        android:id="@+id/button5"
        android:layout_width="295dp"
        android:layout_height="68dp"
        android:background="@drawable/concert"
